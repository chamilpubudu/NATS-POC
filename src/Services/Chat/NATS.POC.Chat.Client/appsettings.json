{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      { "Name": "Console" }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "NATS": "Information",
        "System": "Warning"
      }
    }
  },
  "NatsConnectionString": "nats://localhost:4222",
  "RedisConnectionString": "localhost:6379",
  "Username": "client",
  "AllowedHosts": "*"
}
